<%= form_for @artist, html: { multipart: true } do |f| %>
<div class="form-group">
    <label>Artist Name</label>
    <%= f.text_field :name %>
</div>

<div class="form-group">
    <label>Photos</label>
    <span class="btn btn-default">
        Upload Photos
        <%= file_field_tag "images[]", type: :file, multiple: true %>
    </span>
</div>

<div class="actions">
    <%= f.submit (@artist.new_record? ? "Create" : "Update"), class: "btn btn-primary" %>
    <%= link_to "Back", song_path, class: "btn btn-default" %>
</div>
<% end %>
