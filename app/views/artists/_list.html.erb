
<div id="filterrific_results">


  <table>
    <tr>
      <th>Name</th>
    </tr>
    <% artists.each do |artist| %>
      <tr>
        <td><%= link_to artist.name, artist_path(artist.id) %></td>
    <% artist.photos.each do |photo| %>
        <td><%= image_tag photo.image_url %> </td>
      </tr>
    <% end %>
    <% end %>
  </table>
</div>
